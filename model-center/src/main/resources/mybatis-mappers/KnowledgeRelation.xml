<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.model.dao.KnowledgeRelationDao">

    <select id="getRelationInfo" parameterType="map" resultType="map">
        SELECT
        *
        FROM
        knowledge_relation
        WHERE
        1 = 1
        AND knowledge_code = #{knowledgeCode}
        AND relation_knowledge = #{relationKnowledge}
    </select>



</mapper>